evance`\n        await axios.get(queryString)\n            .then(response => {\n                if(response.status == 200) {\n                    setSearchResults(response.data.query.search)\n                    setEmptyResults(false)\n                } else {\n                    setEmptyResults(true)\n                }\n            })            \n    }    \n    handleSearchUpdate('hockey')\n    const clearResults = () => {\n        setSearchResults([])\n        setEmptyResults(true)\n    }\n    const updateSearchTerms = (newInput) => {\n        setSearchTerms(newInput)\n    }\n\n    return (\n        <WrapperDivLayout>\n            <ControlsLayout>\n                <ControlsContainer>\n                    <SearchBar updateAction={updateSearchTerms} />\n                    <Button \n                        text='Search'\n                        color='#f2f2f2'\n                        backgroundColor='#2a2a2a'\n                        clickAction={handleSearchUpdate}\n                        actionParameter={searchTerms}\n                    />\n                    <Button                        \n                        text='Clear'\n                        color='#2a2a2a'\n                        backgroundColor='#f2f2f2'\n                        clickAction={clearResults}                        \n                    />\n                </ControlsContainer>\n            </ControlsLayout>\n            <ResultsLayout>\n                {\n                    emptyResults ?\n                        <div>To view results, use the search bar (above) then click 'Search'</div>\n                    :\n                        <Results searchData={searchResults} />\n                }            \n            </ResultsLayout>\n        </WrapperDivLayout>\n    )\n}\n\nexport default StateWrapperComponent","/Users/justinjudd/Desktop/wikipedia_finder/src/components/Tooltip.js",[],"/Users/justinjudd/Desktop/wikipedia_finder/src/components/Results.js",[],"/Users/justinjudd/Deskto